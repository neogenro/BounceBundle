  "keywords": ["aws", "ses", "sns", "bounce", "complaints", "blacklist", "swiftmailer"],
  "homepage": "http://www.neogen.biz",
  "license": "MIT",
  "authors": [
    {
      "name": "Bodor Szilard",
      "email": "szilard@neogen.biz"
    }
  ],
  "require": {
    "php": ">=5.3.3",
    "aws/aws-sdk-php": "^3.2",
    "aws/aws-php-sns-message-validator": "*",
    "doctrine/orm": "~2.2,>=2.2.3,<2.6",
    "symfony/framework-bundle": "~2.7|~3.0|^3.4|^4.0",
    "symfony/swiftmailer-bundle": "^3.1",
    "php-http/client-implementation": "^1.0"
  },
  "require-dev": {
    "php-http/guzzle6-adapter": "~1.0",
    "php-http/httplug-bundle": "~1.0"
